{
  "manifest_version": 2,
  "name": "Playback Speed",
  "description": "Use number keys to change playback rate in video",
  "version": "1.0",
  "options_page": "options.html",
  "content_scripts":[
      {
        "run_at": "document_start",
        "matches":[
          "https://www.youtube.com/*"
        ],
        "js": ["content.js"]
      }
    ]
}